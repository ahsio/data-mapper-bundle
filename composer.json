{
    "name":"kassko/data-mapper-bundle",
    "authors": [
        {
            "name":"Kassko"
        }
    ],
    "description":"Integrates data-mapper in Symfony projects.",
    "keywords":[
        "orm", "hydration", "hydrate", "extract", "data", "access",
        "mapper", "dao", "entity", "repository"
    ],
    "require":{
        "php":">=5.5.0",
        "doctrine/common": "~2.4",
        "symfony/framework-bundle": "~2.4",
        "symfony/twig-bundle": "~2.4",
        "zendframework/zend-stdlib": "~2.3",
        "kassko/data-mapper": "~0.6.0@alpha",
        "kassko/class-resolver-bundle": "~0.1@alpha"
    },
    "require-dev":{
        "phpunit/phpunit": "~4.4",
        "mockery/mockery": "~0.9"
    },
    "autoload":{
        "psr-4":{
            "Kassko\\Bundle\\DataMapperBundle\\": "src"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8.0.x-dev"
        }
    }
}
